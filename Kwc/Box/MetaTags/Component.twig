<!-- {{ kwfUp }}metaTags -->
{% for name, content in metaTags %}
    {% set attribute = name|slice(0, 3) == 'og:' ? 'property' : 'name' %}
    <meta {{ attribute }}="{{ name }}" content="{{ content }}" />
{% endfor %}
<link href="{{ canonicalUrl }}" rel="canonical" />
{% for key in keys %}
    {{ renderer.component(_context[key]) }}
{% endfor %}
<!-- /{{ kwfUp }}metaTags -->
